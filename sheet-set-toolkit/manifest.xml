<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<OfficeApp xmlns="http://schemas.microsoft.com/office/appforoffice/1.1" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:bt="http://schemas.microsoft.com/office/officeappbasictypes/1.0" xmlns:ov="http://schemas.microsoft.com/office/taskpaneappversionoverrides" xsi:type="TaskPaneApp">
  <Id>98a67a7c-00a2-4ca4-9632-a545eff3c183</Id>
  <Version>1.0.0.0</Version>
  <ProviderName>Urban Systems</ProviderName>
  <DefaultLocale>en-US</DefaultLocale>
  <DisplayName DefaultValue="Sheet Set Toolkit"/>
  <Description DefaultValue="Tools for editing AutoCAD Sheet Set data in Excel. Supports date insertion, text case conversion, subset isolation, revision management, and more."/>
  <IconUrl DefaultValue="https://urbansys.github.io/dpt/sheet-set-toolkit/assets/icon-32.png"/>
  <HighResolutionIconUrl DefaultValue="https://urbansys.github.io/dpt/sheet-set-toolkit/assets/icon-64.png"/>
  <SupportUrl DefaultValue="https://urbansys.github.io/dpt/sheet-set-toolkit/support.html"/>
  <AppDomains>
    <AppDomain>https://urbansys.github.io/dpt/sheet-set-toolkit</AppDomain>
  </AppDomains>
  <Hosts>
    <Host Name="Workbook"/>
  </Hosts>
  <DefaultSettings>
    <SourceLocation DefaultValue="https://urbansys.github.io/dpt/sheet-set-toolkit/taskpane.html"/>
  </DefaultSettings>
  <Permissions>ReadWriteDocument</Permissions>
  <VersionOverrides xmlns="http://schemas.microsoft.com/office/taskpaneappversionoverrides" xsi:type="VersionOverridesV1_0">
    <Hosts>
      <Host xsi:type="Workbook">
        <DesktopFormFactor>
          <GetStarted>
            <Title resid="GetStarted.Title"/>
            <Description resid="GetStarted.Description"/>
            <LearnMoreUrl resid="GetStarted.LearnMoreUrl"/>
          </GetStarted>
          <ExtensionPoint xsi:type="PrimaryCommandSurface">
            <OfficeTab id="TabHome">
              <Group id="CommandsGroup">
                <Label resid="CommandsGroup.Label"/>
                <Icon>
                  <bt:Image size="16" resid="Icon.16x16"/>
                  <bt:Image size="32" resid="Icon.32x32"/>
                  <bt:Image size="80" resid="Icon.80x80"/>
                </Icon>
                <Control xsi:type="Button" id="TaskpaneButton">
                  <Label resid="TaskpaneButton.Label"/>
                  <Supertip>
                    <Title resid="TaskpaneButton.Label"/>
                    <Description resid="TaskpaneButton.Tooltip"/>
                  </Supertip>
                  <Icon>
                    <bt:Image size="16" resid="Icon.16x16"/>
                    <bt:Image size="32" resid="Icon.32x32"/>
                    <bt:Image size="80" resid="Icon.80x80"/>
                  </Icon>
                  <Action xsi:type="ShowTaskpane">
                    <TaskpaneId>ButtonId1</TaskpaneId>
                    <SourceLocation resid="Taskpane.Url"/>
                  </Action>
                </Control>
              </Group>
            </OfficeTab>
          </ExtensionPoint>
        </DesktopFormFactor>
      </Host>
    </Hosts>
    <Resources>
      <bt:Images>
        <bt:Image id="Icon.16x16" DefaultValue="https://urbansys.github.io/dpt/sheet-set-toolkit/assets/icon-16.png"/>
        <bt:Image id="Icon.32x32" DefaultValue="https://urbansys.github.io/dpt/sheet-set-toolkit/assets/icon-32.png"/>
        <bt:Image id="Icon.80x80" DefaultValue="https://urbansys.github.io/dpt/sheet-set-toolkit/assets/icon-80.png"/>
      </bt:Images>
      <bt:Urls>
        <bt:Url id="GetStarted.LearnMoreUrl" DefaultValue="https://go.microsoft.com/fwlink/?LinkId=276812"/>
        <bt:Url id="Taskpane.Url" DefaultValue="https://urbansys.github.io/dpt/sheet-set-toolkit/taskpane.html"/>
        <bt:Url id="Commands.Url" DefaultValue="https://urbansys.github.io/dpt/sheet-set-toolkit/commands.js"/>
      </bt:Urls>
      <bt:ShortStrings>
        <bt:String id="GetStarted.Title" DefaultValue="Get started with Sheet Set Toolkit!"/>
        <bt:String id="CommandsGroup.Label" DefaultValue="Sheet Set"/>
        <bt:String id="TaskpaneButton.Label" DefaultValue="Sheet Set Toolkit"/>
      </bt:ShortStrings>
      <bt:LongStrings>
        <bt:String id="GetStarted.Description" DefaultValue="Sheet Set Toolkit loaded successfully. Go to the HOME tab and click the 'Toolkit' button to open the task pane."/>
        <bt:String id="TaskpaneButton.Tooltip" DefaultValue="Open Sheet Set Toolkit task pane with tools for editing AutoCAD Sheet Set data"/>
      </bt:LongStrings>
    </Resources>
  </VersionOverrides>
</OfficeApp>